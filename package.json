        "prettier": "^2.4.1",
        "sass": "^1.43.2",
        "sass-loader": "^12.2.0",
        "source-map-support": "^0.5.20",
        "storybook-ngrx-helper": "^0.2.0",
        "style-loader": "^3.3.1",
        "stylelint": "^14.0.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard-scss": "^2.0.1",
        "ts-node": "^10.3.0",
        "typescript": "~4.2.3"
    },
    "lint-staged": {
        "*.{js,ts,html}": [
            "eslint --fix"
        ],
        "*.scss": [
            "stylelint --fix"
        ]
    },
    "msw": {
        "workerDirectory": "src"
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "all": true
    }
